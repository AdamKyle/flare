{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "beyondcode/laravel-websockets": "dev-possible-fix-for-symfony-upgrade",
        "bkwld/cloner": "^3.11.0",
        "christianessl/landmap-generation": "^1.0",
        "deployer/deployer": "v7.2.0",
        "doctrine/dbal": "^3.1",
        "guzzlehttp/guzzle": "^7.0.1",
        "http-interop/http-factory-guzzle": "^1.0",
        "knplabs/github-api": "^3.0",
        "laravel/framework": "^10.0",
        "laravel/helpers": "^1.6",
        "laravel/horizon": "^5.14.2",
        "laravel/tinker": "^2.8.1",
        "laravel/ui": "^4.2.1",
        "league/commonmark": "^2.3",
        "league/fractal": "^0.20.1",
        "maatwebsite/excel": "^3.1",
        "pusher/pusher-php-server": "7.0.2",
        "spatie/laravel-collection-macros": "^7.12.3",
        "spatie/laravel-permission": "^5.9.1",
        "spatie/laravel-short-schedule": "^1.5"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.13.0",
        "beyondcode/laravel-dump-server": "1.9.0",
        "fakerphp/faker": "^1.21.0",
        "laravel/browser-kit-testing": "^7.0",
        "mockery/mockery": "^1.6.6",
        "nunomaduro/collision": "^7.0.5",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "php-http/discovery": false
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/AdamKyle/laravel-websockets.git"
        }
    ],
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi",
            "php artisan horizon:publish"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "phpunit": "./vendor/bin/phpunit --coverage-html=./test-coverage"
    }
}
